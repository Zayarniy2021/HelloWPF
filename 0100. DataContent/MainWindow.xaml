<Window x:Class="_0100.DataContent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_0100.DataContent"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label HorizontalAlignment="Center">Datacontet</Label>
        <Button Content="{Binding Path=RandomValue, TargetNullValue=NULL, FallbackValue=Random}" MinHeight="40" Click="Button_Click"/>
    </StackPanel>
</Window>
